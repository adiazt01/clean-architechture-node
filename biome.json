{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"enabled": true,
			"attributePosition": "auto",
			"indentStyle": "tab",
			"indentWidth": 2,
			"lineWidth": 40,
			"lineEnding": "lf"
		},
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	}
}
